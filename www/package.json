{
  "name": "www",
  "version": "0.0.0-dev",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "prebuild": "deno task fetch-releases:build",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "fetch-releases": "deno task fetch-releases",
    "fetch-releases:build": "deno run --allow-read --allow-write --allow-env --allow-net ../scripts/fetch/fetch-releases.ts build ./public",
    "mock:versions": "cp public/versions.json.example public/versions.json"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "next": "16.0.0"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "16.0.0"
  }
}
